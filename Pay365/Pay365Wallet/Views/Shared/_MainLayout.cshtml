            ns: { namespaces: listResource, defaultNs: 'common.homepage' } //khai báo các file resource
        },
            function (t) {
                $(".translang").i18n();
            });

        $(function () {
            //header.BindTemp();
            $(document).click(function (event) {
                var $target = $(event.target);
                if (!$target.is('.material-icons') && !$target.is('.btn-notifications') && !$('.notifications-section').has($target).length && $('.notifications-section').is(':visible')) {
                    $(".notifications-section").parent().removeClass("notifications-section-open");
                    $("#overpage").removeClass("overlay");
                }
            });
        });

        $('#txt_keyword_search').keyup(function (event) {
            var keyCode = event.keyCode || event.which;
            if (keyCode === 13) {
                news.Btn_SearchNews();
            }
        });

        $(window).scroll(function() {
            topBar();
        });

        $('#btn-top, #explode').on('click', function () {
            if (this.id === 'btn-top')
                goToByScroll('body');
            else
                goToByScroll('#why-use');
        });

        $(window).resize(function () {
            initHeightColumn();
            sideBar();
        });
        $(window).on('load', function () {
            initHeightColumn();
            sideBar();
        });
    </script>
</body>
</html>
